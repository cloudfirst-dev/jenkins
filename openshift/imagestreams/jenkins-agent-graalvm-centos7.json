{
  "kind": "ImageStream",
  "apiVersion": "v1",
  "metadata": {
    "name": "jenkins-agent-graalvm",
    "annotations": {
      "openshift.io/display-name": "Jenkins Maven and Graalvm Agent"
    }
  },
  "spec": {
    "tags": [
      {
        "name": "latest",
        "annotations": {
          "openshift.io/display-name": "Jenkins Maven and Graalvm Agent",
          "description": "Provides a Jenkins Agent with Maven and Graalvm tooling",
          "iconClass": "icon-jenkins",
          "tags": "jenkins"
        },
        "from": {
          "kind": "DockerImage",
          "name": "quay.io/cloudfirst/agent-graalvm:latest"
        },
        "referencePolicy": {
          "type": "Local"
        }
      }
    ]
  }
}